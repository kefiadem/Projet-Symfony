<div class="toast-container position-fixed top-0 start-50 translate-middle-x mt-5 p-3" style="z-index: 1080;">
    <div id="wishlistToastAdded" class="toast align-items-center text-bg-success border-0" role="alert" aria-live="assertive" aria-atomic="true">
        <div class="d-flex">
            <div class="toast-body">Product added to your wishlist!</div>
            <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast" aria-label="Close"></button>
        </div>
    </div>
</div>

<div class="toast-container position-fixed top-0 start-50 translate-middle-x mt-5 p-3" style="z-index: 1080;">
    <div id="wishlistToastExists" class="toast align-items-center text-bg-warning border-0" role="alert" aria-live="assertive" aria-atomic="true">
        <div class="d-flex">
            <div class="toast-body">Product already in your wishlist!</div>
            <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast" aria-label="Close"></button>
        </div>
    </div>
</div>

<script>
    function showWishlistToast(type) {
        let toastId = null;

        if (type === 'added') {
            toastId = 'wishlistToastAdded';
        } else if (type === 'exists') {
            toastId = 'wishlistToastExists';
        }

        if (toastId) {
            const toastEl = document.getElementById(toastId);
            if (toastEl) {
                const toast = new bootstrap.Toast(toastEl, { delay: 2000 });
                toast.show();
            }
        }
    }

    document.addEventListener('DOMContentLoaded', function () {
        document.querySelectorAll('.add-to-wishlist').forEach(button => {
            button.addEventListener('click', function (e) {
                e.preventDefault();
                const productId = this.dataset.productId;

                fetch('/wishlist/add', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/x-www-form-urlencoded',
                        'X-Requested-With': 'XMLHttpRequest'
                    },
                    body: new URLSearchParams({ productId })
                })
                    .then(response => response.json())
                    .then(data => {
                        if (data.status === 'added') {
                            showWishlistToast('added');
                        } else if (data.status === 'exists') {
                            showWishlistToast('exists');
                        }
                        // You can handle other statuses here if needed
                    });
            });
        });
    });
</script>
<script>
    document.addEventListener('DOMContentLoaded', function () {
        const toggle = document.getElementById('wishlistToggle');
        const wishlistOffcanvas = document.getElementById('wishlistOffcanvas');

        if (toggle && wishlistOffcanvas) {
            const bsOffcanvas = new bootstrap.Offcanvas(wishlistOffcanvas);

            toggle.addEventListener('click', function (e) {
                e.preventDefault();
                bsOffcanvas.show();
            });

            wishlistOffcanvas.addEventListener('show.bs.offcanvas', function () {
                fetch('/wishlist')
                    .then(res => res.json())
                    .then(data => {
                        let html = '';
                        if (data.length === 0) {
                            html = '<p>Your wishlist is empty.</p>';
                        } else {
                            data.forEach(item => {
                                let imgPath = item.image || '';
                                if (imgPath.startsWith('/uploads/assets/')) {
                                    imgPath = imgPath.replace('/uploads/assets/', '/assets/');
                                }

                                html += `
            <div class="wishlist-item d-flex align-items-center mb-3 bg-dark text-white p-3 rounded" style="min-height: 100px;">

                <!-- Image -->
                <img src="${imgPath}" alt="${item.name}" class="rounded me-3" style="width: 80px; height: 80px; object-fit: cover;">

                <!-- Name + Add to Cart Button -->
                <div class="flex-grow-1">
                <h5 class="mb-2">${item.name}</h5>
                <button class="btn btn-outline-success btn-sm rounded add-to-cart" data-id="${item.id}">
                Add to Cart
                </button>
                </div>

                <!-- Remove Button (X Icon) -->
                <button class="btn btn-outline-light btn-sm ms-3 remove-wishlist" data-id="${item.id}">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-x" viewBox="0 0 16 16">
                    <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1
                    .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646
                    2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                </svg>
                </button>

            </div>
            `;
                            });
                        }
                        document.getElementById('wishlistContent').innerHTML = html;
                    });
            });

            document.addEventListener('click', function (e) {
                const removeBtn = e.target.closest('button.remove-wishlist');
                if (removeBtn && wishlistOffcanvas.contains(removeBtn)) {
                    const id = removeBtn.getAttribute('data-id');
                    fetch(`/wishlist/remove/${id}`, {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                            'X-Requested-With': 'XMLHttpRequest',
                            'X-CSRF-TOKEN': document.querySelector('meta[name="csrf-token"]').content
                        },
                        body: JSON.stringify({})
                    }).then(() => {
                        const event = new Event('show.bs.offcanvas');
                        wishlistOffcanvas.dispatchEvent(event);
                    });
                }
            });
        }
    });
    document.addEventListener('click', function (e) {
        const addBtn = e.target.closest('button.add-to-cart');
        if (addBtn && wishlistOffcanvas.contains(addBtn)) {
            const id = addBtn.getAttribute('data-id');

            fetch(`/cart/add/${id}`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                    'X-Requested-With': 'XMLHttpRequest',
                    'X-CSRF-TOKEN': document.querySelector('meta[name="csrf-token"]').content
                },
                body: JSON.stringify({})
            }).then(() => {
                // Optionally show a toast or message
                showCartToast();
            });
        }
    });

    function showCartToast() {
        var toastEl = document.getElementById('cartToast');
        if (toastEl) {
            var toast = new bootstrap.Toast(toastEl, { delay: 2500 });
            toast.show();
        }
    }

</script>
